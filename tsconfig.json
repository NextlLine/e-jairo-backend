{
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "allowJs": true,
    "target": "es5",
    "outDir": ".build",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["es2015"],
    "rootDir": "./",
    "types": ["node", "aws-lambda"],
  },
    "include": ["app/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
